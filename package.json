{
  "name": "puranveshana",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:components": "jest --testPathPattern=__tests__/components",
    "test:coverage": "jest --coverage --collectCoverageFrom='app/**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='components/**/*.{js,jsx,ts,tsx}' --collectCoverageFrom='lib/**/*.{js,jsx,ts,tsx}'",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "./tests/lighthouse/run-lighthouse.sh",
    "load-test": "artillery run tests/load/api-load-test.yml",
    "load-test:report": "artillery run tests/load/api-load-test.yml --output tests/load/report.json && artillery report tests/load/report.json",
    "test:all": "npm run test:ci && npm run lighthouse && npm run load-test",
    "test:perf": "npm run lighthouse && npm run load-test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/lib-storage": "^3.940.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.18.0",
    "@react-google-maps/api": "^2.20.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "@types/react-simple-maps": "^3.0.6",
    "@upstash/redis": "^1.36.0",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "exifr": "^7.1.3",
    "jose": "^6.1.0",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "lucide-react": "^0.552.0",
    "next": "16.0.7",
    "nodemailer": "^7.0.10",
    "posthog-js": "^1.313.0",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-leaflet": "^5.0.0",
    "react-simple-maps": "^3.0.0",
    "resend": "^6.5.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/leaflet-routing-machine": "^3.2.9",
    "@types/node": "^20.19.24",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "artillery": "^2.0.27",
    "artillery-plugin-metrics-by-endpoint": "^1.21.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
